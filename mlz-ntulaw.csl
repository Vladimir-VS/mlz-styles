    </choose>
  </macro>

  <macro name="wisconsin-secondary-first">
    <group delimiter=": ">
      <choose>
        <if type="video">
          <text form="short" variable="medium"/>
        </if>
      </choose>
      <group delimiter=", ">
        <group delimiter=" ">
          <choose>
            <if match="any" type="book classic">
              <number variable="volume"/>
            </if>
          </choose>
          <group delimiter=", ">
            <text macro="us-default-author"/>
            <choose>
              <if>
                <conditions match="any">
                  <condition type="entry-encyclopedia entry-dictionary" match="none"/>
                  <condition variable="author" match="any"/>
                </conditions>
                <text macro="amlaw-title"/>
              </if>
            </choose>
            <text macro="us-interview-author"/>
            <text macro="us-collection-title-podcast"/>
            <text macro="opus-number"/>
            <text macro="us-default-presentation-description"/>
            <choose>
              <if type="webpage post manuscript">
                <text form="short" variable="genre"/>
              </if>
              <else-if type="report">
                <group delimiter=" ">
                  <text variable="genre"/>
                  <number variable="number"/>
                </group>
              </else-if>
            </choose>
            <group delimiter=" ">
              <text macro="us-default-chapter-to-book"/>
              <choose>
                <if match="none" type="book classic">
                  <number variable="volume"/>
                </if>
              </choose>
              <group delimiter=", ">
                <text font-variant="small-caps" macro="us-container-author"/>
                <text macro="us-container-title"/>
              </group>
              <choose>
                <if type="entry-encyclopedia entry-dictionary">
                  <choose>
                    <if match="none" variable="author">
                      <text font-style="italic" variable="title"/>
                    </if>
                  </choose>
                </if>
              </choose>
            </group>
            <choose>
              <if match="any" type="article-newspaper article-magazine">
                <choose>
                  <if has-year-only="issued">
                    <group delimiter=" ">
                      <label form="symbol" variable="issue"/>
                      <number variable="issue"/>
                    </group>
                  </if>
                </choose>
                <date date-parts="year-month-day" form="text" variable="issued"/>
                <choose>
                  <if match="none" variable="URL">
                    <choose>
                      <if is-numeric="edition">
                        <group delimiter=" ">
                          <number form="ordinal" variable="edition"/>
                          <label form="short" variable="edition"/>
                        </group>
                      </if>
                      <else>
                        <number variable="edition"/>
                      </else>
                    </choose>
                    <group delimiter=" ">
                      <choose>
                        <if is-numeric="section">
                          <text form="symbol" term="section"/>
                        </if>
                      </choose>
                      <text variable="section"/>
                    </group>
                  </if>
                </choose>
                <group delimiter=" ">
                  <group delimiter=" ">
                    <text term="at"/>
                    <group delimiter=" ">
                      <group delimiter=", ">
                        <number variable="page-first"/>
                        <choose>
                          <if locator="page">
                            <number variable="locator"/>
                          </if>
                        </choose>
                      </group>
                      <text macro="std-locator-label"/>
                    </group>
                  </group>
                  <choose>
                    <if variable="jurisdiction">
                      <group delimiter=" " prefix="(" suffix=")">
                        <text macro="std-authority-full"/>
                        <date date-parts="year-month-day" form="text" variable="original-date"/>
                      </group>
                    </if>
                  </choose>
                </group>
              </if>
              <else-if>
                <conditions match="all">
                  <condition is-numeric="locator" match="none"/>
                  <condition type="book" variable="title" match="all"/>
                </conditions>
                <text label-form="symbol" macro="std-locator-non-page-label-at"/>
              </else-if>
              <else-if>
                <conditions match="any">
                  <condition match="all" type="song" variable="section"/>
                  <condition is-numeric="title" match="all" type="song" variable="title locator"/>
                  <condition is-numeric="title" match="all" type="book" variable="title locator"/>
                </conditions>
                <text macro="us-pinpoint-action"/>
              </else-if>
              <else-if>
                <conditions match="all">
                  <condition variable="volume"/>
                  <condition match="none" variable="container-title"/>
                </conditions>
                <text macro="us-pinpoint-action"/>
              </else-if>
            </choose>
          </group>
          <choose>
            <if match="none" type="article-newspaper article-magazine">
              <choose>
                <if>
                  <conditions match="all">
                    <condition match="all" type="report"/>
                    <condition match="none" has-day="issued"/>
                  </conditions>
                </if>
                <else>
                  <group delimiter=", ">
                    <number variable="page-first"/>
                    <choose>
                      <if variable="page">
                        <text macro="us-pinpoint-action"/>
                      </if>
                    </choose>
                    <text macro="us-pinpoint-action"/>
                  </group>
                </else>
              </choose>
              <text macro="us-issued"/>
              <choose>
                <if>
                  <conditions match="all">
                    <condition match="all" type="report"/>
                    <condition match="none" has-day="issued"/>
                  </conditions>
                  <text macro="us-pinpoint-action"/>
                </if>
              </choose>
              <text macro="us-default-thesis-details"/>
            </if>
          </choose>
        </group>
        <choose>
          <if match="all" type="report" has-day="issued">
            <group delimiter=" ">
              <text value="at"/>
              <number variable="page-first"/>
            </group>
            <text macro="std-locator-non-page-label"/>
          </if>
        </choose>
        <choose>
          <if type="graphic post-weblog webpage post entry-dictionary entry-encyclopedia broadcast motion_picture video song map">
            <group delimiter=" ">
              <choose>
                <if type="post-weblog webpage post entry-dictionary entry-encyclopedia"/>
                <else-if match="all" type="song" variable="collection-title">
                  <choose>
                    <if variable="container-title">
                      <text font-style="italic" value="available at"/>
                    </if>
                  </choose>
                </else-if>
                <else-if variable="publisher">
                  <text font-style="italic" value="available at"/>
                </else-if>
              </choose>
              <text variable="URL"/>
            </group>
          </if>
          <else-if match="all" type="book" variable="version">
            <text variable="URL"/>
          </else-if>
          <else-if type="article-newspaper">
            <text variable="URL"/>
          </else-if>
        </choose>
      </group>
    </group>
  </macro>



  <!-- macros for en bib from apa.csl -->

  <macro name="apa-container-contributors">
    <choose>
      <if type="chapter paper-conference" match="any">
        <text term="in" text-case="capitalize-first" suffix=" "/>
        <names variable="editor" delimiter=", " suffix=", ">
          <name and="symbol" initialize-with=". " delimiter=", "/>
          <label form="short" prefix=" (" text-case="capitalize-first" suffix=")"/>
          <substitute>
            <names variable="translator"/>
          </substitute>
        </names>
      </if>
    </choose>
  </macro>
  <macro name="apa-secondary-contributors">
    <choose>
      <if type="chapter paper-conference" match="none">
        <names variable="translator" delimiter=", " prefix=" (" suffix=")">
          <name and="symbol" initialize-with=". " delimiter=", "/>
          <label form="short" prefix=", " text-case="capitalize-first" suffix=""/>
          <substitute>
            <names variable="editor"/>
          </substitute>
        </names>
      </if>
    </choose>
  </macro>
  <macro name="apa-author">
    <names variable="author">
      <name name-as-sort-order="all" and="symbol" sort-separator=", " initialize-with=". " delimiter=", " delimiter-precedes-last="always"/>
      <label form="short" prefix=" (" suffix=".)" text-case="capitalize-first" strip-periods="true"/>
      <substitute>
        <names variable="editor"/>
        <names variable="translator"/>
        <choose>
          <if type="report">
            <text variable="publisher"/>
            <text macro="apa-title"/>
          </if>
          <else>
            <text macro="apa-title"/>
          </else>
        </choose>
      </substitute>
    </names>
  </macro>
  <macro name="apa-access">
    <choose>
      <if type="thesis">
        <choose>
          <if variable="archive" match="any">
            <group>
              <text term="retrieved" text-case="capitalize-first" suffix=" "/>
              <text term="from" suffix=" "/>
              <text variable="archive" suffix="."/>
              <text variable="archive_location" prefix=" (" suffix=")"/>
            </group>
          </if>
          <else>
            <group>
              <text term="retrieved" text-case="capitalize-first" suffix=" "/>
              <text term="from" suffix=" "/>
              <text variable="URL"/>
            </group>
          </else>
        </choose>
      </if>
      <else>
        <choose>
          <if variable="DOI">
            <text variable="DOI" prefix="doi:"/>
          </if>
          <else>
            <choose>
              <if type="webpage">
                <group delimiter=" ">
                  <text term="retrieved" text-case="capitalize-first" suffix=" "/>
                  <group>
                    <date variable="accessed" suffix=", ">
                      <date-part name="month" suffix=" "/>
                      <date-part name="day" suffix=", "/>
                      <date-part name="year"/>
                    </date>
                  </group>
                  <text term="from"/>
                  <text variable="URL"/>
                </group>
              </if>
              <else>
                <group>
                  <text term="retrieved" text-case="capitalize-first" suffix=" "/>
                  <text term="from" suffix=" "/>
                  <text variable="URL"/>
                </group>
              </else>
            </choose>
          </else>
        </choose>
      </else>
    </choose>
  </macro>
  <macro name="apa-title">
    <choose>
      <if type="report thesis" match="any">
        <text variable="title" font-style="italic"/>
        <group prefix=" (" suffix=")">
          <text variable="genre"/>
          <text variable="number" prefix=" No. "/>
        </group>
      </if>
      <else-if type="book graphic  motion_picture report song manuscript speech" match="any">
        <text variable="title" font-style="italic"/>
      </else-if>
      <else>
        <text variable="title"/>
      </else>
    </choose>
  </macro>
  <macro name="apa-publisher">
    <choose>
      <if type="report" match="any">
        <group delimiter=": ">
          <text variable="publisher-place"/>
          <text variable="publisher"/>
        </group>
      </if>
      <else-if type="thesis" match="any">
        <group delimiter=", ">
          <text variable="publisher"/>
          <text variable="publisher-place"/>
        </group>
      </else-if>
      <else>
        <group delimiter=", ">
          <choose>
            <if variable="event" match="none">
              <text variable="genre"/>
            </if>
          </choose>
          <choose>
            <if type="article-journal article-magazine" match="none">
              <group delimiter=": ">
                <text variable="publisher-place"/>
                <text variable="publisher"/>
              </group>
            </if>
          </choose>
        </group>
      </else>
    </choose>
  </macro>
  <macro name="apa-event">
    <choose>
      <if variable="event">
        <choose>
          <if variable="genre" match="none">
            <text term="presented at" text-case="capitalize-first" suffix=" "/>
            <text variable="event"/>
          </if>
          <else>
            <group delimiter=" ">
              <text variable="genre" text-case="capitalize-first"/>
              <text term="presented at"/>
              <text variable="event"/>
            </group>
          </else>
        </choose>
      </if>
    </choose>
  </macro>
  <macro name="apa-issued">
    <choose>
      <if type="bill legal_case legislation" match="none">
        <choose>
          <if variable="issued">
            <group prefix=" (" suffix=").">
              <date variable="issued">
                <date-part name="year"/>
              </date>
              <text variable="year-suffix"/>
              <choose>
                <if type="article-journal bill book chapter graphic legal_case legislation motion_picture paper-conference report song" match="none">
                  <date variable="issued">
                    <date-part prefix=", " name="month"/>
                    <date-part prefix=" " name="day"/>
                  </date>
                </if>
              </choose>
            </group>
          </if>
          <else>
            <group prefix=" (" suffix=").">
              <text term="no date" form="short"/>
              <text variable="year-suffix" prefix="-"/>
            </group>
          </else>
        </choose>
      </if>
    </choose>
  </macro>
  <macro name="apa-issued-sort">
    <choose>
      <if type="article-journal bill book chapter graphic legal_case legislation motion_picture paper-conference report song" match="none">
        <date variable="issued">
          <date-part name="year"/>
          <date-part name="month"/>
          <date-part name="day"/>
        </date>
      </if>
      <else>
        <date variable="issued">
          <date-part name="year"/>
        </date>
      </else>
    </choose>
  </macro>
  <macro name="apa-edition">
    <choose>
      <if is-numeric="edition">
        <group delimiter=" ">
          <number variable="edition" form="ordinal"/>
          <text term="edition" form="short" suffix="." strip-periods="true"/>
        </group>
      </if>
      <else>
        <text variable="edition" suffix="."/>
      </else>
    </choose>
  </macro>
  <macro name="apa-locators">
    <choose>
      <if type="article-journal article-magazine" match="any">
        <group prefix=", " delimiter=", ">
          <group>
            <text variable="volume" font-style="italic"/>
            <text variable="issue" prefix="(" suffix=")"/>
          </group>
          <text variable="page"/>
        </group>
      </if>
      <else-if type="article-newspaper">
        <group delimiter=" " prefix=", ">
          <label variable="page" form="short"/>
          <text variable="page"/>
        </group>
      </else-if>
      <else-if type="book graphic motion_picture report song chapter paper-conference" match="any">
        <group prefix=" (" suffix=")" delimiter=", ">
          <text macro="apa-edition"/>
          <group>
            <text term="volume" form="short" plural="true" text-case="capitalize-first" suffix=". " strip-periods="true"/>
            <number variable="number-of-volumes" form="numeric" prefix="1-"/>
          </group>
          <group>
            <text term="volume" form="short" text-case="capitalize-first" suffix=". " strip-periods="true"/>
            <number variable="volume" form="numeric"/>
          </group>
          <group>
            <label variable="page" form="short" suffix=" "/>
            <text variable="page"/>
          </group>
        </group>
      </else-if>
      <else-if type="legal_case">
        <group prefix=" (" suffix=")" delimiter=" ">
          <names variable="authority"/>
          <date variable="issued" delimiter=" ">
            <date-part name="month" form="short"/>
            <date-part name="day" suffix=","/>
            <date-part name="year"/>
          </date>
        </group>
      </else-if>
      <else-if type="bill legislation">
        <date variable="issued" prefix=" (" suffix=")">
          <date-part name="year"/>
        </date>
      </else-if>
    </choose>
  </macro>
  <macro name="apa-citation-locator">
    <group>
      <label variable="locator" form="short"/>
      <text variable="locator" prefix=" "/>
    </group>
  </macro>
  <macro name="apa-container">
    <choose>
      <if type="bill legal_case legislation" match="none">
        <text variable="container-title" font-style="italic"/>
      </if>
      <else>
        <group delimiter=" " prefix=", ">
          <choose>
            <if variable="container-title">
              <text variable="volume"/>
              <text variable="container-title"/>
              <group delimiter=" ">
                <!--change to label variable="section" as that becomes available -->
                <text term="section" form="symbol"/>
                <text variable="section"/>
              </group>
              <text variable="page"/>
            </if>
            <else>
              <choose>
                <if type="legal_case">
                  <text variable="number" prefix="No. "/>
                </if>
                <else>
                  <text variable="number" prefix="Pub. L. No. "/>
                  <group delimiter=" ">
                    <!--change to label variable="section" as that becomes available -->
                    <text term="section" form="symbol"/>
                    <text variable="section"/>
                  </group>
                </else>
              </choose>
            </else>
          </choose>
        </group>
      </else>
    </choose>
  </macro>



  <!-- macros for zh citation -->

  <macro name="std-locator-zh">
    <choose>
      <if locator="page">
        <number variable="locator" prefix="頁"/>
      </if>
      <else-if locator="paragraph">
        <number variable="locator" prefix="第" suffix="段"/>
      </else-if>
      <else-if locator="chapter">
        <number variable="locator" prefix="第" suffix="章"/>
      </else-if>
      <else-if locator="article">
        <number variable="locator" prefix="第" suffix="條"/>
      </else-if>
      <else-if locator="section">
        <choose>
          <if type="bill legislation regulation">
            <number variable="locator" prefix="第" suffix="條"/>
          </if>
          <else>
            <number variable="locator" prefix="第" suffix="節"/>
          </else>
        </choose>
      </else-if>
      <else>
        <text variable="locator"/>
      </else>
    </choose>
  </macro>


  <macro name="tw-container-author">
    <names variable="container-author">
      <name and="text" delimiter-precedes-last="never" initialize="false" initialize-with="."/>
      <institution institution-parts="short" use-first="1" use-last="1"/>
    </names>
  </macro>

  <macro name="tw-author">
    <choose>
      <if type="interview">
        <group delimiter="">
          <names variable="author">
            <name and="symbol" delimiter="" initialize="false" initialize-with="."/>
            <institution and="text" delimiter=", " text-case="title" form="short" institution-parts="short" use-first="1" use-last="1"/>
          </names>
          <text variable="medium"/>
          <text form="verb" term="interview"/>
        </group>
      </if>
      <else-if type="broadcast" match="none">
        <names variable="author">
          <name and="text" delimiter-precedes-last="never" form="long" initialize="false" initialize-with="."/>
          <substitute>
            <names variable="container-author"/>
            <names variable="composer"/>
          </substitute>
        </names>
      </else-if>
    </choose>
  </macro>

  <macro name="tw-author-short">
    <choose>
      <if type="interview">
        <names variable="author">
          <name and="text" delimiter="" initialize="false" initialize-with="."/>
          <institution substitute-use-first="1" use-last="0"/>
        </names>
      </if>
      <else-if type="broadcast" match="none">
        <names variable="author">
          <name and="text" delimiter-precedes-last="never" form="long" initialize="false" initialize-with="."/>
          <substitute>
            <names variable="container-author"/>
            <names variable="composer"/>
          </substitute>
        </names>
      </else-if>
    </choose>
  </macro>

  <macro name="tw-title-version">
    <group delimiter=", ">
      <choose>
        <if is-numeric="version">
          <text variable="version" prefix="第" suffix="版" />
          <text form="short" variable="medium"/>
        </if>
        <else>
          <text variable="version"/>
          <text form="short" variable="medium"/>
        </else>
      </choose>
    </group>
  </macro>

  <macro name="tw-default-title">
    <choose>
      <if type="book song motion_picture">
        <choose>
          <if match="all" variable="title version">
            <group delimiter=", ">
              <text variable="title"/>
              <choose>
                <if disambiguate="true">
                  <text macro="tw-title-version"/>
                </if>
                <else-if position="subsequent"/>
                <else>
                  <text macro="tw-title-version"/>
                </else>
              </choose>
            </group>
          </if>
          <else-if variable="container-title">
            <group>
              <text text-case="title" variable="title"/>
              <names prefix="【" suffix="】" variable="composer"/>
            </group>
          </else-if>
          <else>
            <text variable="title"/>
          </else>
        </choose>
      </if>
      <else-if match="any" type="broadcast video">
        <choose>
          <if match="all" variable="container-title publisher">
            <group delimiter=": ">
              <text variable="container-title"/>
              <text variable="title"/>
            </group>
          </if>
          <else>
            <text variable="title"/>
          </else>
        </choose>
      </else-if>
      <else-if type="entry-encyclopedia entry-dictionary" match="any">
        <choose>
          <if variable="author">
            <text variable="title"/>
          </if>
        </choose>
      </else-if>
      <else>
        <text variable="title"/>
      </else>
    </choose>
  </macro>

  <macro name="tw-title-short">
    <choose>
      <if match="none" variable="author composer editor container-author">
        <text macro="tw-title-short-format-select"/>
      </if>
      <else-if type="motion_picture broadcast">
        <text macro="tw-title-short-format-select"/>
      </else-if>
      <else-if disambiguate="true">
        <text macro="tw-title-short-format-select"/>
      </else-if>
    </choose>
  </macro>

  <macro name="tw-title-short-format-select">
    <choose>
      <if variable="hereinafter">
        <text variable="hereinafter"/>
      </if>
      <else-if variable="title-short">
        <text form="short" variable="title"/>
      </else-if>
      <else>
        <text macro="tw-default-title"/>
      </else>
    </choose>
  </macro>

  <macro name="zh-editor">
    <choose>
      <if type="broadcast">
        <group>
          <names delimiter="，" variable="author">
            <name delimiter-precedes-last="never"/>
          </names>
          <text term="director"/>
        </group>
      </if>
      <else>
        <names delimiter="，" variable="director editor translator">
          <name delimiter-precedes-last="never"/>
          <label form="short"/>
        </names>
      </else>
    </choose>
  </macro>

  <macro name="zh-edition">
    <group delimiter="">
      <number form="long-ordinal" variable="edition"/>
      <text term="edition"/>
    </group>
  </macro>

  <macro name="tw-issued">
    <group>
      <choose>
        <if type="classic"/>
        <else-if type="manuscript">
          <text macro="tw-issued-content"/>
        </else-if>
        <else>
          <text macro="tw-issued-content" prefix=" (" suffix=")"/>
        </else>
      </choose>
    </group>
  </macro>

  <macro name="tw-issued-content">
    <group delimiter="，">
      <choose>
        <if type="graphic song broadcast">
          <text variable="medium"/>
        </if>
      </choose>
      <choose>
        <if type="graphic">
          <text variable="dimensions"/>
        </if>
        <else-if type="song broadcast video">
          <text variable="genre"/>
        </else-if>
      </choose>
      <names variable="interviewer" prefix="訪談者："/>
      <text macro="zh-editor"/>
      <choose>
        <if type="song"/>
        <else-if type="thesis">
            <text variable="genre"/>
            <text variable="publisher"/>
        </else-if>
        <else-if variable="collection-title">
          <text variable="collection-title"/>
          <group>
            <choose>
              <if is-numeric="collection-number">
                <label form="long" variable="volume"/>
              </if>
            </choose>
            <number label-form="short" variable="collection-number"/>
          </group>
        </else-if>
      </choose>
      <group>
        <text term="in"/>
        <text form="short" variable="archive"/>
      </group>
      <text variable="archive_location"/>
      <group delimiter="，">
        <group delimiter="，">
          <choose>
            <if match="all" position="subsequent" type="periodical"/>
            <else-if match="none" type="thesis">
              <group delimiter="，" subgroup-delimiter="；">
                <choose>
                  <if type="chapter book manuscript" match="none">
                    <text variable="publisher-place"/>
                  </if>
                </choose>
                <text macro="zh-edition"/>
                <choose>
                  <if match="none" position="first">
                    <text form="short" variable="publisher"/>
                  </if>
                </choose>
              </group>
            </else-if>
          </choose>
        </group>
        <choose>
          <if match="none" variable="locator-revision">
            <text macro="tw-issued-date"/>
          </if>
        </choose>
      </group>
    </group>
  </macro>

  <macro name="tw-issued-date">
    <choose>
      <if match="any" has-year-only="issued" type="legal_case">
        <date date-parts="year" form="numeric" variable="issued"/>
      </if>
      <else>
        <date date-parts="year-month-day" form="text" variable="issued"/>
      </else>
    </choose>
  </macro>

  <macro name="ibid-with-locator-label-zh">
        <group delimiter="，">
          <text term="ibid"/>
          <text macro="std-locator-zh"/>
        </group>
  </macro>

  <macro name="hereinafter-zh">
    <choose>
      <if variable="hereinafter title-short">
        <group prefix=" (" suffix=")">
          <text value="以下簡稱"/>
          <choose>
            <if variable="hereinafter">
              <text variable="hereinafter" prefix="「" suffix="」"/>
            </if>
            <else>
              <text variable="title-short" prefix="「" suffix="」"/>
            </else>
          </choose>
        </group>
      </if>
    </choose>
  </macro>

  <macro name="hereinafter-stub-zh">
    <choose>
      <if variable="hereinafter">
        <text variable="hereinafter"/>
      </if>
      <else>
        <text variable="title-short"/>
      </else>
    </choose>
  </macro>

  <macro name="tw-case-first">
    <group delimiter="">
      <text variable="title"/>
      <text macro="hereinafter-zh"/>
      <text macro="tw-issued-date" prefix=" (" suffix=")"/>
      <text macro="std-locator-zh" prefix=" (" suffix=")"/>
    </group>
  </macro>

  <macro name="tw-case-subsequent">
      <group delimiter=", ">
          <text macro="hereinafter-stub-zh"/>
          <text macro="tw-supra-stub"/>
      </group>
  </macro>

  <macro name="tw-bill-subsequent">
    <group delimiter=", ">
      <choose>
        <if variable="hereinafter title-short">
          <text macro="hereinafter-stub-zh"/>
        </if>
        <else>
          <text variable="title"/>
        </else>
      </choose>
      <text macro="tw-supra-stub"/>
    </group>
  </macro>

  <macro name="tw-bill-first">
    <group>
      <group delimiter="，">
        <group>
          <text variable="title"/>
          <text macro="hereinafter-zh"/>
        </group>
        <text macro="std-locator-zh"/>
      </group>
      <text macro="tw-issued-date" prefix=" (" suffix=")"/>
    </group>
  </macro>

  <macro name="tw-legislation-subsequent">
    <group delimiter="">
      <choose>
        <if variable="hereinafter title-short">
          <text macro="hereinafter-stub-zh"/>
        </if>
        <else>
          <text variable="title"/>
        </else>
      </choose>
      <text macro="std-locator-zh"/>
    </group>
  </macro>

  <macro name="tw-legislation-first">
    <group delimiter="">
      <group>
        <text variable="title"/>
        <text macro="hereinafter-zh"/>
      </group>
      <text macro="std-locator-zh"/>
    </group>
  </macro>

  <macro name="tw-supra-stub">
    <group delimiter="，">
      <text variable="first-reference-note-number" prefix="前揭註"/>
      <text macro="std-locator-zh"/>
    </group>
  </macro>

  <macro name="jurisdictions-case-zh">
    <choose>
      <if position="subsequent">
        <text macro="tw-case-subsequent"/>
      </if>
      <else>
        <text macro="tw-case-first"/>
      </else>
    </choose>
  </macro>

  <macro name="jurisdictions-bill-zh">
    <choose>
      <if position="subsequent">
        <text macro="tw-bill-subsequent"/>
      </if>
      <else>
        <text macro="tw-bill-first"/>
      </else>
    </choose>
  </macro>

  <macro name="jurisdictions-legislation-zh">
    <choose>
      <if position="subsequent">
        <text macro="tw-legislation-subsequent"/>
      </if>
      <else>
        <text macro="tw-legislation-first"/>
      </else>
    </choose>
  </macro>

  <macro name="tw-container-title">
    <group>
      <choose>
        <if match="all" type="song" variable="title container-title">
          <text term="in"/>
        </if>
      </choose>
      <choose>
        <if type="broadcast"/>
        <else-if match="all" type="video" variable="publisher"/>
        <else>
          <text form="short" variable="container-title"/>
        </else>
      </choose>
    </group>
  </macro>

  <macro name="tw-default-presentation-description">
    <choose>
      <if match="all" type="paper-conference" variable="container-title event"/>
      <else-if variable="event">
        <choose>
          <if variable="title">
            <text term="presented at"/>
          </if>
          <else-if variable="genre">
            <text form="short" variable="genre"/>
          </else-if>
          <else>
            <text term="presented at"/>
          </else>
        </choose>
        <text text-case="title" variable="event"/>
      </else-if>
    </choose>
  </macro>

  <macro name="jurisdiction-filter-zh">
    <choose>
      <else-if type="article-newspaper">
        <choose>
          <if position="subsequent">
            <text macro="tw-secondary-subsequent"/>
          </if>
          <else>
            <text macro="tw-secondary-first"/>
          </else>
        </choose>
      </else-if>
      <else-if type="legal_case">
        <text macro="jurisdictions-case-zh"/>
      </else-if>
      <else-if type="bill">
        <text macro="jurisdictions-bill-zh"/>
      </else-if>
      <else-if match="any" type="legislation">
        <text macro="jurisdictions-legislation-zh"/>
      </else-if>
      <else-if type="hearing">
        <text macro="jurisdictions-hearing"/>
      </else-if>
      <else-if type="report">
        <text macro="jurisdictions-report"/>
      </else-if>
      <else-if type="treaty">
        <text macro="jurisdictions-treaty"/>
      </else-if>
      <else-if type="patent">
        <text macro="jurisdictions-patent"/>
      </else-if>
      <else-if position="subsequent">
        <text macro="tw-secondary-subsequent"/>
      </else-if>
      <else>
        <text macro="tw-secondary-first"/>
      </else>
    </choose>
  </macro>

  <macro name="tw-collection-title-podcast">
    <choose>
      <if type="song">
        <choose>
          <if match="none" variable="container-title">
            <text variable="collection-title"/>
          </if>
        </choose>
      </if>
    </choose>
  </macro>

  <macro name="tw-secondary-subsequent">
    <group delimiter="，">
      <text macro="tw-author-short"/>
      <text macro="tw-title-short"/>
      <text macro="tw-supra-stub"/>
    </group>
  </macro>

  <macro name="tw-secondary-first">

    <group delimiter="，">
      <group>
        <group delimiter="，">
          <group delimiter="，">
            <text macro="tw-author"/>
            <text macro="tw-default-title"/>
            <text macro="tw-collection-title-podcast"/>
            <text macro="opus-number"/>
            <text macro="tw-default-presentation-description"/>
            <choose>
              <if type="webpage post manuscript">
                <text form="short" variable="genre"/>
              </if>
            </choose>
            <group>
              <choose>
                <if type="chapter">
                  <text term="in"/>
                </if>
              </choose>
              <choose>
                <if match="all" type="chapter" variable="container-author">
                  <choose>
                    <if match="none" variable="author">
                      <text macro="tw-container-title" prefix="氏著「" suffix="」" />
                    </if>
                    <else>
                      <group delimiter="，">
                        <text macro="tw-container-author"/>
                        <text macro="tw-container-title"/>
                      </group>
                    </else>
                  </choose>
                </if>
                <else>
                  <group delimiter="，">
                    <text macro="tw-container-author"/>
                    <group>
                      <text macro="tw-container-title"/>
                      <choose>
                        <if type="article-newspaper">
                          <choose>
                            <if match="any" variable="edition section">
                              <group delimiter="，" prefix=" (" suffix=")">
                                <choose>
                                  <if is-numeric="edition">
                                    <number form="long-ordinal" variable="edition" suffix="版"/>
                                  </if>
                                  <else>
                                    <text variable="edition"/>
                                  </else>
                                </choose>
                                <choose>
                                  <if is-numeric="section">
                                    <text variable="section" prefix="第" suffix="落"/>
                                  </if>
                                  <else>
                                    <text variable="section"/>
                                  </else>
                                </choose>
                              </group>
                            </if>
                          </choose>
                        </if>
                      </choose>
                    </group>
                  </group>
                </else>
              </choose>
              <group>
                <number variable="volume" suffix="卷"/>
                <number variable="issue" suffix="期"/>
              </group>
              <choose>
                <if type="entry-encyclopedia entry-dictionary">
                  <choose>
                    <if match="none" variable="author">
                      <text variable="title"/>
                    </if>
                  </choose>
                </if>
              </choose>
            </group>
            <choose>
              <if type="article-newspaper">
                <date date-parts="year-month-day" form="text" variable="issued"/>
                <group delimiter=", ">
                  <number variable="page-first" suffix="版"/>
                  <text macro="std-locator-zh"/>
                </group>
              </if>
              <else-if variable="locator">
                <text macro="std-locator-zh"/>
              </else-if>
              <else-if match="all" type="song" variable="section">
                <text macro="std-locator-zh"/>
              </else-if>
              <else-if is-numeric="title" match="all" type="song" variable="title locator">
                <text macro="std-locator-zh"/>
              </else-if>
              <else-if variable="volume">
                <choose>
                  <if match="none" variable="container-title">
                    <text macro="std-locator-zh"/>
                  </if>
                </choose>
              </else-if>
            </choose>
          </group>

          <choose>
            <if match="none" type="article-newspaper">
              <choose>
                <if match="none" position="first">
                  <group prefix="頁">
                    <number variable="page"/>
                  </group>
                </if>
                <else>
                  <number variable="page-first" prefix="頁"/>
                  <text macro="std-locator-zh"/>
                </else>
              </choose>
            </if>
          </choose>
        </group>
        <choose>
          <if match="none" type="article-newspaper">
            <text macro="tw-issued"/>
          </if>
        </choose>
      </group>

      <choose>
        <if type="graphic post-weblog webpage post entry-dictionary entry-encyclopedia broadcast motion_picture video song map">
          <group delimiter=" ">
            <choose>
              <if type="post-weblog webpage post entry-dictionary entry-encyclopedia"/>
              <else-if match="all" type="song" variable="collection-title">
                <choose>
                  <if variable="container-title">
                    <text value="亦見於"/>
                  </if>
                </choose>
              </else-if>
              <else-if variable="publisher">
                <text value="亦見於"/>
              </else-if>
            </choose>
            <text variable="URL"/>
          </group>
        </if>
        <else-if match="all" type="book" variable="version">
          <text variable="URL"/>
        </else-if>
        <else-if match="all" type="article-newspaper" variable="URL">
          <choose>
            <if variable="page-first">
              <group delimiter=" ">
                <text value="亦見於"/>
                <text variable="URL"/>
              </group>
            </if>
            <else>
              <text variable="URL"/>
            </else>
          </choose>
        </else-if>
      </choose>
    </group>

  </macro>



  <!-- macros for zh bib, based on apa6 -->


  <macro name="apa-container-contributors-zh">
    <choose>
      <if type="chapter paper-conference" match="any">
        <text term="in" text-case="capitalize-first"/>
        <names variable="editor" delimiter="、" suffix="，">
          <name and="text" initialize-with=". " delimiter="、"/>
          <label form="short" prefix=" (" text-case="capitalize-first" suffix=")"/>
          <substitute>
            <names variable="translator"/>
          </substitute>
        </names>
      </if>
    </choose>
  </macro>
  <macro name="apa-secondary-contributors-zh">
    <choose>
      <if type="chapter paper-conference" match="none">
        <names variable="translator" delimiter="、" prefix=" (" suffix=")">
          <name and="text" initialize-with=". " delimiter="、"/>
          <label form="short" prefix="，" text-case="capitalize-first" suffix=""/>
          <substitute>
            <names variable="editor"/>
          </substitute>
        </names>
      </if>
    </choose>
  </macro>
  <macro name="apa-author-zh">
    <names variable="author">
      <name name-as-sort-order="all" and="" sort-separator="，" initialize-with=". " delimiter="、" delimiter-precedes-last="always"/>
      <label form="short" prefix=" (" suffix=".)" text-case="capitalize-first" strip-periods="true"/>
      <substitute>
        <names variable="container-author"/>
        <names variable="editor" suffix="編"/>
        <names variable="translator" suffix="譯"/>
        <choose>
          <if type="report">
            <text variable="publisher"/>
            <text macro="apa-title-zh"/>
          </if>
          <else>
            <text macro="apa-title-zh"/>
          </else>
        </choose>
      </substitute>
    </names>
  </macro>
  <macro name="apa-access-zh">
    <choose>
      <if type="thesis">
        <choose>
          <if variable="archive" match="any">
            <group>
              <text term="retrieved" text-case="capitalize-first" suffix=": "/>
              <text variable="archive" suffix="。"/>
              <text variable="archive_location" prefix=" (" suffix=")"/>
            </group>
          </if>
          <else>
            <group>
              <text term="retrieved" text-case="capitalize-first" suffix=": "/>
              <text variable="URL"/>
            </group>
          </else>
        </choose>
      </if>
      <else>
        <choose>
          <if variable="DOI">
            <text variable="DOI" prefix="doi:"/>
          </if>
          <else>
            <choose>
              <if type="webpage">
                <group delimiter="">
                  <text macro="tw-issued-date"/>
                  <text term="retrieved" text-case="capitalize-first" suffix=": "/>
                  <text variable="URL"/>
                </group>
              </if>
              <else>
                <group>
                  <text term="retrieved" text-case="capitalize-first" suffix=": "/>
                  <text variable="URL"/>
                </group>
              </else>
            </choose>
          </else>
        </choose>
      </else>
    </choose>
  </macro>
  <macro name="apa-title-zh">
    <choose>
      <if type="report thesis" match="any">
        <text variable="title" font-style="italic"/>
        <group prefix=" (" suffix=")">
          <text variable="genre"/>
          <text variable="number" prefix="第" suffix="號"/>
        </group>
      </if>
      <else-if type="book graphic  motion_picture report song manuscript speech" match="any">
        <text variable="title" font-style="italic"/>
      </else-if>
      <else>
        <text variable="title"/>
      </else>
    </choose>
  </macro>
  <macro name="apa-publisher-zh">
    <choose>
      <if type="report" match="any">
        <group delimiter="：">
          <text variable="publisher-place"/>
          <text variable="publisher"/>
        </group>
      </if>
      <else-if type="thesis" match="any">
        <group delimiter="，">
          <text variable="publisher"/>
          <text variable="publisher-place"/>
        </group>
      </else-if>
      <else>
        <group delimiter="，">
          <choose>
            <if variable="event" match="none">
              <text variable="genre"/>
            </if>
          </choose>
          <choose>
            <if type="article-journal article-magazine" match="none">
              <group delimiter="：">
                <text variable="publisher-place"/>
                <text variable="publisher"/>
              </group>
            </if>
          </choose>
        </group>
      </else>
    </choose>
  </macro>
  <macro name="apa-event-zh">
    <choose>
      <if variable="event">
        <choose>
          <if variable="genre" match="none">
            <text term="presented at" text-case="capitalize-first" suffix=" "/>
            <text variable="event"/>
          </if>
          <else>
            <group delimiter="">
              <text variable="genre" text-case="capitalize-first"/>
              <text term="presented at"/>
              <text variable="event"/>
            </group>
          </else>
        </choose>
      </if>
    </choose>
  </macro>
  <macro name="apa-issued-zh">
    <choose>
      <if type="bill legal_case legislation" match="none">
        <choose>
          <if variable="issued">
            <group prefix=" (" suffix=")。">
              <date date-parts="year" form="numeric" variable="issued"/>
              <choose>
                <if type="article-journal bill book chapter graphic legal_case legislation motion_picture paper-conference report song" match="none">
                  <date variable="issued">
                    <date-part prefix="，" name="month" form="numeric" suffix="月" />
                    <date-part prefix="" name="day" form="numeric" suffix="日" />
                  </date>
                </if>
              </choose>
            </group>
          </if>
          <else>
            <group prefix=" (" suffix=")。">
              <text term="no date" form="short"/>
              <text variable="year-suffix" prefix="-"/>
            </group>
          </else>
        </choose>
      </if>
    </choose>
  </macro>
  <macro name="apa-locators-zh">
    <choose>
      <if type="article-journal article-magazine" match="any">
        <group prefix="，" delimiter="，">
          <group>
            <number variable="volume" suffix="卷"/>
            <number variable="issue" suffix="期"/>
          </group>
          <text variable="page" prefix="頁"/>
        </group>
      </if>
      <else-if type="article-newspaper">
        <group delimiter="" prefix="，">
          <text variable="page" suffix="版"/>
        </group>
      </else-if>
      <else-if type="book graphic motion_picture report song chapter paper-conference" match="any">
        <group prefix=" (" suffix=")" delimiter="，">
          <text macro="zh-edition"/>
          <number variable="number-of-volumes" form="numeric" prefix="共1-" suffix="卷"/>
          <number variable="volume" form="numeric" prefix="第" suffix="卷"/>
          <text variable="page" prefix="頁"/>
        </group>
      </else-if>
      <else-if type="bill legislation legal_case">
        <date variable="issued" prefix=" (" suffix=")">
          <date-part name="year"/>
        </date>
      </else-if>
    </choose>
  </macro>
  <macro name="apa-citation-locator-zh">
    <group>
      <label variable="locator" form="short"/>
      <text variable="locator" prefix=" "/>
    </group>
  </macro>
  <macro name="apa-container-zh">
    <choose>
      <if type="bill legal_case legislation" match="none">
        <text variable="container-title" font-style="italic"/>
      </if>
      <else>
        <group delimiter=" " prefix=", ">
          <choose>
            <if variable="container-title">
              <text variable="container-title"/>
              <group>
                <text variable="volume" prefix="第" suffix="章"/>
                <text variable="section" prefix="第" suffix="節"/>
              </group>
              <text variable="page" prefix="頁"/>
            </if>
          </choose>
        </group>
      </else>
    </choose>
  </macro>






  <citation disambiguate-add-givenname="true" et-al-min="3" et-al-use-first="1" near-note-distance="5">

    <layout locale="zh" delimiter="；" suffix="。">
      <choose>
        <if position="ibid-with-locator">
          <text macro="ibid-with-locator-label-zh"/>
        </if>
        <else-if position="ibid">
          <text term="ibid"/>
        </else-if>
        <else-if match="any" type="treaty" variable="jurisdiction">
          <text macro="jurisdiction-filter-zh"/>
        </else-if>
        <else-if match="none" position="first" type="classic">
          <text macro="tw-secondary-subsequent"/>
        </else-if>
        <else>
          <text macro="tw-secondary-first"/>
        </else>
      </choose>
    </layout>

    <layout delimiter="; " suffix=".">
      <choose>
        <if match="any" type="treaty" variable="jurisdiction">
          <text macro="jurisdiction-filter"/>
        </if>
        <else-if position="ibid-with-locator">
          <text macro="ibid-with-locator-label"/>
        </else-if>
        <else-if position="ibid">
          <text font-style="italic" term="ibid"/>
        </else-if>
        <else-if match="none" position="first" context="bibliography" type="classic">
          <text macro="wisconsin-secondary-subsequent"/>
        </else-if>
        <else>
          <text macro="wisconsin-secondary-first"/>
        </else>
      </choose>
    </layout>

  </citation>


  <bibliography hanging-indent="true" et-al-min="4" et-al-use-first="3" entry-spacing="0" line-spacing="2">
    <sort>
      <key macro="apa-author"/>
      <key macro="apa-issued-sort" sort="ascending"/>
    </sort>

    <layout locale="zh">
      <choose>
        <if type="legislation"/>
        <!-- else-if match="any" type="treaty" variable="jurisdiction">
          <text macro="jurisdiction-filter-zh"/>
        </else-if -->
        <else>
          <group suffix="。">
            <group delimiter="。">
              <text macro="apa-author-zh"/>
              <text macro="apa-issued-zh"/>
            </group>
            <group delimiter="。">
              <text macro="apa-title-zh" prefix=""/>
              <group>
                <text macro="apa-container-contributors-zh"/>
                <text macro="apa-secondary-contributors-zh"/>
                <group delimiter="，">
                  <text macro="apa-container-zh"/>
                  <text variable="collection-title"/>
                </group>
              </group>
            </group>
            <text macro="apa-locators-zh"/>
            <group delimiter="，" prefix="。">
              <text macro="apa-event-zh"/>
              <text macro="apa-publisher-zh"/>
            </group>
          </group>
          <text macro="apa-access-zh" prefix=""/>
        </else>
      </choose>
    </layout>

    <layout>
      <group suffix=".">
        <group delimiter=". ">
          <text macro="apa-author"/>
          <text macro="apa-issued"/>
        </group>
        <group delimiter=". ">
          <text macro="apa-title" prefix=" "/>
          <group>
            <text macro="apa-container-contributors"/>
            <text macro="apa-secondary-contributors"/>
            <group delimiter=", ">
              <text macro="apa-container"/>
              <text variable="collection-title"/>
            </group>
          </group>
        </group>
        <text macro="apa-locators"/>
        <group delimiter=", " prefix=". ">
          <text macro="apa-event"/>
          <text macro="apa-publisher"/>
        </group>
      </group>
      <text macro="apa-access" prefix=" "/>
    </layout>

  </bibliography>


</style>
