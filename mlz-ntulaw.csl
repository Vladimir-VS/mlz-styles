          </group>
          <group delimiter=" ">
            <text term="note"/>
            <text variable="first-reference-note-number"/>
          </group>
        </group>
        <group delimiter=" ">
          <choose>
            <if locator="page">
              <text term="at"/>
            </if>
            <else>
              <label form="short" variable="locator"/>
            </else>
          </choose>
          <number variable="locator"/>
        </group>
      </group>
  </macro>

